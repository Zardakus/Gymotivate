@model ConquistasModel
@{
    ViewData["Title"] = "Conquistas";
}

<head>
    <style>

        main {
            justify-content: center;
            align-items: center;
            padding-top: 0px;
        }

        .conquista-container {
            display: flex;
            flex-direction: column;
            padding: 0px 10px 10px 0px;
            width: 100%;
            max-width: 800px;
            min-width: 700px;
            flex-shrink: 0;
            border-radius: 10px;
            background: #1A1C1A;
            box-shadow: 0px 0px 0px 1px rgba(255, 255, 255, 0.00) inset, 0px 2px 8px 0px #000, 0px 1px 1px 0px #000;
        }

        .tipo {
            color: #48D34E;
            font-family: Patua One;
            font-size: 28px;
            font-style: normal;
            font-weight: 400;
            line-height: normal;
            margin: 0px;
        }

        .circulo {
            fill: rgba(0, 0, 0, 0.00);
            stroke-width: 1px;
            stroke: rgba(72, 211, 78, 0.45);
            padding-left: 5px;
        }

        .conquista-esquerda {
            display: flex;
            width: 100%;
            max-width: 200px;
            height: 47px;
            flex-shrink: 0;
            border-radius: 5px;
            border: 1px solid rgba(72, 211, 78, 0.50);
            justify-content: flex-start;
            align-items: center;
            box-shadow: 0px 0px 0px 0px rgba(255, 255, 255, 0.00) inset, 0px 1px 8px 0px #000, 0px 1px 1px 0px #000;
        }

            .conquista-esquerda p {
                margin: 0;
                padding-left: 20px;
            }

        .texto-claro {
            display:flex;
            color: #B7F2B9;
            font-family: Patua One;
            font-size: 14px;
            font-style: normal;
            font-weight: 400;
        }

        .conquista-direita {
            display: flex;
        }

            .conquista-direita div {
                width: 80px;
                height: 100px;
                flex-shrink: 0;
                border: 1px dashed rgba(72, 211, 78, 0.40);
                background: rgba(0, 0, 0, 0.00);
                margin-right: 10px;
            }

        .conquista {
            display: flex;
            justify-content: space-evenly;
        }

        .titulo {
            display: flex;
            padding-top: 10px;
            padding-left: 23px;
        }

        .linha {
            height: 0.5px;
            background: rgba(72, 211, 78, 0.50);
            margin: 15px 10px;
        }

        .qtd-conquista {
            padding-top: 10px;
        }

        .conquista-titulo {
            display: inline-flex;
            padding: 0px 52px 18px 46px;
            align-items: center;
            border-radius: 0px 0px 15px 15px;
            background: #00282A;
            box-shadow: 0px -2px 6px 0px rgba(72, 211, 78, 0.35) inset, 0px 25px 60px -30px rgba(72, 211, 78, 0.30), 0px 50px 100px -20px rgba(72, 211, 78, 0.25);
        }

            .conquista-titulo p {
                color: #006330;
                font-family: Karla;
                font-size: 36px;
                font-style: normal;
                font-weight: 700;
                line-height: normal;
            }

        .conquista-direita svg {
            display: flex;
            width:100%;
            height:100%;
            vertical-align: middle;
        }

        svg#icone-conquista {
            fill: red;
        }

        svg title {
            display: none; /* Inicialmente, ocultar a dica de ferramenta */
            position: absolute; /* Posição absoluta para sobrepor o SVG */
            background-color: #333; /* Cor de fundo da dica de ferramenta */
            color: #fff; /* Cor do texto da dica de ferramenta */
            padding: 5px; /* Preenchimento da dica de ferramenta */
            border-radius: 5px; /* Bordas arredondadas */
            z-index: 1; /* Colocar a dica de ferramenta acima do SVG */
        }

        svg:hover title {
        display: block; /* Exibir a dica de ferramenta quando o mouse estiver sobre o SVG */
        }

        svg {
            background: rgba(0, 150, 136, 0.3);
            border-radius: 20px;
            box-shadow: inset 0 0 15px rgba(0, 0, 0, 0.5);
        }

    }

    </style>
</head>

